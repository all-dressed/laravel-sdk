{
    "name": "all-dressed/laravel-sdk",
    "description": "Laravel SDK for All Dressed API.",
    "keywords": [
        "foodtech",
        "laravel-sdk"
    ],
    "homepage": "https://github.com/all-dressed/laravel-sdk",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Chin Leung",
            "email": "chin@swoo.ca",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "AllDressed\\Laravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AllDressed\\Laravel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AllDressed\\Laravel\\AllDressedServiceProvider"
            ]
        }
    }
}
